{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/en/youtube/(.*)",
      "dest": "/"
    },
    {
      "src": "/en/youtube",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/en/youtube/$1"
    }
  ],
  "rewrites": [
    { "source": "/en/youtube/(.*)", "destination": "/$1" },
    { "source": "/en/youtube", "destination": "/" }
  ]
}
